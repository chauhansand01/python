def is_prime(number):
    if number <= 1:
        return False
    if number == 2:
        return True
    if number % 2 == 0:
        return False

    max_divisor = int(number**0.5) + 1
    for divisor in range(3, max_divisor, 2):
        if number % divisor == 0:
            return False
    return True

N = int(input("Enter the value of N: "))

print(f"prime numbers from 1 to {N} are:")

for num in range(2, N + 1):
    if is_prime(num):
        print(num)
